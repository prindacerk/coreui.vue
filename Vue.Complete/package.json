{
  "name": "coreui.vue.complete",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build:dev-vendor": "webpack --config webpack.config.vendor.js",
    "build:dev": "yarn run build:dev-vendor && webpack",
    "build:prod-vendor": "webpack --config webpack.config.vendor.js --env.prod",
    "build:prod": "yarn run build:prod-vendor && webpack --env.prod",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.1",
    "@types/toastr": "^2.1.35",
    "@types/webpack-env": "^1.13.5",
    "aspnet-webpack": "^2.0.3",
    "awesome-typescript-loader": "^5.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "event-source-polyfill": "^0.0.12",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "typescript": "^2.8.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "vue-loader": "^14.2.2",
    "vue-property-decorator": "^6.0.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-clean-plugin": "^0.2.3",
    "webpack-hot-middleware": "^2.22.0"
  },
  "dependencies": {
    "bootstrap": "^4.1.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "simple-line-icons": "^2.4.1",
    "toastr": "^2.1.4",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vue-toastr": "^2.0.12"
  }
}
