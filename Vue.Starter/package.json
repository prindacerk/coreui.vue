{
  "name": "coreui.vue.starter",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build:dev-vendor": "webpack --config webpack.config.vendor.js",
    "build:dev": "yarn run build:dev-vendor && webpack",
    "build:prod-vendor": "webpack --config webpack.config.vendor.js --env.prod",
    "build:prod": "yarn run build:prod-vendor && webpack --env.prod",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.2",
    "@types/toastr": "^2.1.35",
    "@types/webpack-env": "^1.13.6",
    "aspnet-webpack": "^3.0.0",
    "awesome-typescript-loader": "^5.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "event-source-polyfill": "^0.0.12",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "typescript": "^2.9.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.2.4",
    "vue-property-decorator": "^6.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.3",
    "webpack-clean-plugin": "^0.2.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-dev-middleware": "^3.1.3"
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "simple-line-icons": "^2.4.1",
    "toastr": "^2.1.4",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vue-toastr": "^2.0.14"
  }
}
